{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/build/app.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}/build",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": true,
			"sourceMaps": false,
			"outDir": "${workspaceRoot}/build"
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5681,
			"sourceMaps": false,
			"outDir": "${workspaceRoot}/build",
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		}
	]
}